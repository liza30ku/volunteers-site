{
  "type": "Product",
  "selection": {
    "p1": "it.creatorCode",
    "p2": "it.code.$max"
  },
  "distinct": true,
  "cond": "it.$id $in ['${product1Id}', '${product2Id}', '${product3Id}', '${product4Id}', '${product5Id}']",
  "group": "it.creatorCode",
  "groupCond": "it.creatorCode != 2",
  "sort": "it.creatorCode",
  "limit": 2,
  "count": true
}