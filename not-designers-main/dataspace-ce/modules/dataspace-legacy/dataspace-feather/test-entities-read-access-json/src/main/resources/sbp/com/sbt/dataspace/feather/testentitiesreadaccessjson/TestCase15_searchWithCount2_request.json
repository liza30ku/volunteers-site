{
  "merge": [
    {
      "type": "TestEntity",
      "props": [
        "p1",
        "p2"
      ],
      "cond": "1 == 0"
    },
    {
      "type": "Product",
      "props": {
        "relatedProduct": {
          "props": "code"
        },
        "services": {
          "props": "code",
          "cond": "elem.code $like 'service%'"
        }
      },
      "cond": "root.$id == '${product2Id}'"
    },
    {
      "type": "Service",
      "props": "code",
      "cond": "1 == 0"
    }
  ],
  "count": true
}