    <property dbms="oracle" name="type_double" value="BINARY_DOUBLE"/>
    <property dbms="oracle" name="type_float" value="FLOAT"/>
    <property dbms="oracle" name="type_int" value="NUMBER(10,0)"/>
    <property dbms="oracle" name="type_long" value="NUMBER(19,0)"/>
    <property dbms="oracle" name="type_short" value="NUMBER(5,0)"/>
    <property dbms="oracle" name="type_byte" value="NUMBER(3,0)"/>
    <property dbms="oracle" name="type_boolean" value="NUMBER(1,0)"/>
    <property dbms="oracle" name="type_blob" value="BLOB"/>
    <property dbms="oracle" name="type_offsetDateTime0" value="TIMESTAMP(0) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime1" value="TIMESTAMP(1) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime2" value="TIMESTAMP(2) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime3" value="TIMESTAMP(3) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime4" value="TIMESTAMP(4) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime5" value="TIMESTAMP(5) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="type_offsetDateTime6" value="TIMESTAMP(6) WITH LOCAL TIME ZONE"/>
    <property dbms="oracle" name="postfix_type" value="CHAR"/>
    <property dbms="oracle" name="index_parallel_count" value="1"/>
    <property dbms="oracle" name="parallelDegree" value="${index_parallel_count}"/>
    <property dbms="oracle" name="liquibase.oracle.online" value=""/>
    <property dbms="oracle" name="default_schema_name" value=""/>
    <property dbms="postgresql" name="schemaname" value="${user_name}"/>
    <property dbms="postgresql" name="namecase" value="lower"/>
    <property dbms="postgresql" name="bytelength" value="length"/>
    <property dbms="postgresql" name="systimestamp" value="localtimestamp"/>
